<Project Sdk="Microsoft.NET.Sdk">
  <Target Name="AutoVersion" AfterTargets="AfterBuild">
  <PropertyGroup>
    <PackageVersion>$(GitSemVerMajor).$(GitSemVerMinor).$(GitSemVerPatch)$(GitSemVerDashLabel)</PackageVersion>	
  </PropertyGroup>	 
  </Target>
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	<RootNamespace>reexmonkey.xmisc.backbone.io.jil</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GitInfo" Version="2.0.8">
	 <PrivateAssets>All</PrivateAssets>
	</PackageReference>
    <PackageReference Include="Jil" Version="2.15.4" />
    <PackageReference Include="xmisc.core.io" Version="4.0.1-alpha" />
  </ItemGroup>

</Project>
