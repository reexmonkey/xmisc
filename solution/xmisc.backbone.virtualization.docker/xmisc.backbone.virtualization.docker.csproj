<Project Sdk="Microsoft.NET.Sdk">
  <Target Name="AutoVersion" AfterTargets="AfterBuild">
  <PropertyGroup>
    <PackageVersion>$(GitSemVerMajor).$(GitSemVerMinor).$(GitSemVerPatch)$(GitSemVerDashLabel)</PackageVersion>	
  </PropertyGroup>	 
  </Target>
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	<RootNamespace>reexmonkey.xmisc.backbone.virtualization.docker</RootNamespace>
	<Authors>reexmonkey</Authors>
  </PropertyGroup>

  <ItemGroup>
   <PackageReference Include="Docker.DotNet" Version="3.125.2" />
   <PackageReference Include="GitInfo" Version="2.0.26">
	 <PrivateAssets>All</PrivateAssets>
	</PackageReference>
    <PackageReference Include="xmisc.core.system.net" Version="4.0.64-alpha" />
  </ItemGroup>

</Project>
